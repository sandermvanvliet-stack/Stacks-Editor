{
    "extends": "@stackoverflow/tsconfig/tsconfig.json",
    "compilerOptions": {
        "outDir": "./dist/",
        "strictBindCallApply": true
    },
    "include": ["src/**/*"],
    "typedocOptions": {
        "entryPointStrategy": "Expand",
        "entryPoints": [ "src" ],
        "exclude": "src/index.ts",
        "excludePrivate": true,
        "excludeNotDocumented": true,
        "out": "docs"
    }
}
